{
  "name": "meta-education",
  "version": "0.0.9",
  "description": "Méta Education Dashboard",
  "main": "index.js",
  "scripts": {
    "dev-client": "NODE_ENV=development webpack --watch",
    "dev-server": "node_modules/nodemon/bin/nodemon.js --watch src/ --ignore src/client --ignore src/public/dist -- node_modules/babel/bin/babel-node.js src/app.js",
    "prod": "NODE_ENV=production webpack && NODE_ENV=production node src/app.js",
    "start": "npm run prod",
    "staging": "PORT=4000 npm run prod",
    "publish": "release-it -c release-it.json",
    "doc": "./node_modules/apidoc/bin/apidoc -i src/routes/",
    "doc-init": "npm run doc && cd doc && git init . && git checkout -b gh-pages && git add . && git commit -m \"Deploy\" && git remote add origin \"https://github.com/sical/meta-education.git\" && git push origin gh-pages",
    "doc-deploy": "npm run doc &&  cd doc && git add . && git commit -m \"Deploy\" && git push origin gh-pages"
  },
  "apidoc": {
    "title": "Documentation for MetaEducation web API",
    "version": "0.0.7"
  },
  "author": "Clément Renaud <clement.renaud@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "cytoscape": "^2.7.10",
    "d3-array": "^1.0.2",
    "d3-scale": "^1.0.4",
    "d3-shape": "^1.0.4",
    "d3-time": "^1.0.4",
    "express": "4.13.3",
    "fetch": "^1.1.0",
    "history": "^4.4.0",
    "hogan": "^1.0.2",
    "hogan-express": "^0.5.2",
    "material-ui": "^0.16.3",
    "moment": "^2.16.0",
    "mongojs": "^2.4.0",
    "react": "^15.3.2",
    "react-d3-components": "^0.6.5",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^0.13.0",
    "redux-thunk": "^2.1.0",
    "simple-statistics": "^2.2.0"
  },
  "devDependencies": {
    "babel": "5.8.29",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.25.0",
    "json-loader": "^0.5.4",
    "nodemon": "1.8.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-logger": "^2.7.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
